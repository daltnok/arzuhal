<%= form_for :user, url: users_path do |f| %>


    <p>
      <%= f.label :isim %><br>
      <%= f.text_field :isim %>
    </p>

    <p>
      <%= f.label :soyisim %><br>
      <%= f.text_field :soyisim %>
    </p>
    <p>
      <%= f.label :email %><br>
      <%= f.text_field :email %>
    </p>
    <p>
      <%= f.label :biyografi %><br>
      <%= f.text_area :biyografi %>
    </p>

    <p>
      <%= f.submit %>
    </p>
<% end %>